// UK lang variables

/* Remember to namespace the language parameters lang_<your plugin>_<some name> */

tinyMCELang['lang_spellerpages_title'] = 'SpellChecker';
tinyMCELang['lang_spellerpages_desc'] = 'SpellCheck';
tinyMCELang['lang_spellerpages_inprogress'] = 'Spell check in progress...';

